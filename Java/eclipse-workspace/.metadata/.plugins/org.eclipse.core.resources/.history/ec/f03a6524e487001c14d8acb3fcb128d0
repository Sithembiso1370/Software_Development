package task4;

import java.util.ArrayList;
import java.util.List;

public class NoDups {

	
//	 TODO : Create a new file called NoDups.java

	public static void main(String[] args) {
		int sample_array[] = {10,20,20,30,30,40,50,50};
		
//		TODO 3 : Test your method by calling it from the class main method with an array
//		argument comprising the following elements:
		 for (int i=0; i<sample_array.length; i++)  
	           System.out.print(sample_array[i]+" ");
		 
//		 TODO 2: Test your method by calling it from the class main method with an array
//		 argument comprising the following elements:
		removeDuplicateElements(sample_array, sample_array.length);
		removeDuplicateElements(sample_array, sample_array.length);
		

		System.out.print("\n"); 
//		TODO 3 : Test your method by calling it from the class main method with an array
//		argument comprising the following elements:
		 for (int i=0; i<sample_array.length; i++)  
	           System.out.print(sample_array[i]+" ");  


	}
	
//	 TODO 1: Create a method that takes an integer array argument and removes all duplicates from the array
	public static int removeDuplicateElements(int arr[], int n){  
        if (n==0 || n==1){  
            return n;  
        }  
        
        
        int[] temp = new int[n];  
        int j = 0;  
        
        for (int i=0; i<n-1; i++){  
            if (arr[i] != arr[i+1]){  
                temp[j++] = arr[i];  
            }  
         }
        
        temp[j++] = arr[n-1];     
        // Changing original array  
        for (int i=0; i<j; i++){  
            arr[i] = temp[i];  
        }  
        return j;  
    }  
 

}
